  <!--Dogs Walkers-->
  <div class="container">
    <div class="row justify-content-center ">
      <% @petwalkers.each do |petwalker| %>
        <div class="col-lg-3 mb-3 mx-2 border">
          <div class="box">
              <% if petwalker.photo.attached? %>
                <%= cl_image_tag petwalker.photo.key, class: 'card-img-top', width: 70 %>
              <% else %>
                <!--<i class="fa-solid fa-paw card-img-top"></i>-->
                  <%= image_tag "petwalker.png", style: "border-radius: 5%", class: "card-img-top"%>
              <% end %>

              <h5 class="card-title m-3"></h5>
              <%= link_to petwalker.email, user_path(petwalker) %>
              <p class="card-description">Description</p>
              <%= link_to 'RESERVAR', new_pet_path, class: 'btn btn-primary mb-2' %>
          </div>
        </div>
        <% end %>
    </div>
  </div>

  <!--Footer-->
  <div class="footer text-white bg-primary">
    <p>The walking pet team</p>
  </div>
